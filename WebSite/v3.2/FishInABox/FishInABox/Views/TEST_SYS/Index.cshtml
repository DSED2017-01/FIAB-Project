@{
    ViewData["Title"] = "Import Order";
    ViewData["Page Title"] = "Imports Management System";
    ViewData["currentFile"] = "";
}

<h2>@ViewData["Title"] (Testing)</h2>

<form asp-action="Index" method="get">
    <div class="form-actions no-color">
        <p>
            Excel file to read in : 
            <input name="InputFileName" value="@ViewData["currentFile"]" 
                   type="file" accept=".xls"/>
            <input type="submit" value="Import" class="btn btn-default" /> 
        </p>
    </div>
</form>
